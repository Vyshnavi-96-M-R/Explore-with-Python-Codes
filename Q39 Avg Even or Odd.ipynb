   "source": [
    "# Q1.Take a list of numbers as input. Print whether the average is even or odd.\n",
    "\n",
    "num=list(map(int,input(\"Enter the numbers : \").split()))\n",
    "print(\"Your List is : \",num)\n",
    "avg=sum(num)/len(num)\n",
    "print(\"Average is : \",avg)\n",
    "if avg%2==0:\n",
    "    print(avg,\"is even\")\n",
    "else:\n",
    "    print(avg,\"is odd\")"
   ]
