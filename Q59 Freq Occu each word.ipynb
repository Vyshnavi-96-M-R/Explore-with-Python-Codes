   "source": [
    "# Q6. Take a string as input and find out the most frequently occuring word.\n",
    "\n",
    "str1=input(\"Enter a String : \").split()\n",
    "c=dict()\n",
    "for i in str1:\n",
    "    if i in c:\n",
    "        c[i]+=1\n",
    "    else:\n",
    "        c[i]=1\n",
    "print(\"Most frequently occuring word : \",max(c))"
   ]
  }
